{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo:Async": {
      "Name": "Async",
      "Args": {
        "configure": [
          {
            "Name": "Console"
          },
          {
            "Name": "File",
            "Args": {
              "path": "D:/CommonApiLogs/log.txt",
              "rollingInterval": "Day",
              "fileSizeLimitBytes": 10485760,
              "retainedFileCountLimit": 100,
              "rollOnFileSizeLimit": true,
              "shared": true,
              "flushToDiskInterval": "00:00:01"
            }
          }
        ]
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JWT": {
    "Issuer": "签发方",
    "Audience": "接受方",
    "Key": "A86DA130-1B95-4748-B3B2-1B6AA9F2F743", // 加密密钥
    "ExpireSeconds": 600 // 密钥过期时间
  },
  "EnableRequestResponseLogging": true
}